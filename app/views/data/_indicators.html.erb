<% if indicators.any? %>
<div class="indicators indicators-<%= group.admin_name %>">
  <h2><%= group.public_name %></h2>
  <dl>
    <dt><span>&nbsp;</span></dt>
    <dd class="indicators-label">
    <span class="il0">worst</span>
    <span class="il1">best</span>
    </dd>

    <%= render partial: 'data/indicator', collection: indicators.sort_by { |i| i.source.id } %>

  </dl>
</div>
<% end %>
